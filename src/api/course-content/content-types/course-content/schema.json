{
  "kind": "collectionType",
  "collectionName": "course_contents",
  "info": {
    "singularName": "course-content",
    "pluralName": "course-contents",
    "displayName": "CourseContent"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "courses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::course.course",
      "inversedBy": "course_contents"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "contents": {
      "type": "dynamiczone",
      "components": [
        "shared.media",
        "quiz.quiz"
      ],
      "required": true
    }
  }
}
